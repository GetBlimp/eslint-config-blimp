{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },

  "parserOptions": {
    "ecmaVersion": 2017
  },

  "plugins": ["prefer-let"],

  "rules": {
    "accessor-pairs": 2,
    "array-bracket-newline": 0,
    "array-bracket-spacing": [2, "never"],
    "array-callback-return": 2,
    "array-element-newline": 0,
    "arrow-body-style": 0,
    "arrow-parens": [2, "always"],
    "arrow-spacing": [2, { "after": true, "before": true }],
    "block-scoped-var": 2,
    "block-spacing": 0,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "callback-return": 2,
    "camelcase": 2,
    "capitalized-comments": 0,
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "comma-spacing": 2,
    "comma-style": [2, "last"],
    "complexity": [2, 11],
    "computed-property-spacing": [2, "never"],
    "consistent-return": 0,
    "consistent-this": 0,
    "constructor-super": 2,
    "curly": [2, "all"],
    "default-case": 0,
    "dot-location": 0,
    "dot-notation": [2, { "allowKeywords": true }],
    "eol-last": 2,
    "eqeqeq": 2,
    "for-direction": "error",
    "func-call-spacing": [2, "never"],
    "func-name-matching": 0,
    "func-names": 0,
    "func-style": [0, "declaration"],
    "function-paren-newline": 0,
    "generator-star-spacing": 0,
    "getter-return": "error",
    "global-require": 2,
    "guard-for-in": 0,
    "handle-callback-err": 0,
    "id-blacklist": 0,
    "id-length": 0,
    "id-match": 0,
    "implicit-arrow-linebreak": 0,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "init-declarations": 0,
    "jsx-quotes": 0,
    "key-spacing": [2, { "afterColon": true, "beforeColon": false }],
    "keyword-spacing": 2,
    "line-comment-position": 0,
    "linebreak-style": [2, "unix"],
    "lines-around-comment": 0,
    "lines-around-directive": 0,
    "lines-between-class-members": 0,
    "max-classes-per-file": 0,
    "max-depth": [2, 4],
    "max-len": [2, 80, 4, { "ignoreUrls": true }],
    "max-lines-per-function": 0,
    "max-lines": 0,
    "max-nested-callbacks": [2, 4],
    "max-params": [2, 5],
    "max-statements-per-line": [2, { "max": 1 }],
    "max-statements": [0, 10],
    "multiline-comment-style": 0,
    "multiline-ternary": [2, "never"],
    "new-cap": 0,
    "new-parens": 2,
    "newline-after-var": 0,
    "newline-before-return": 0,
    "newline-per-chained-call": 0,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-async-promise-executor": 0,
    "no-await-in-loop": "off",
    "no-bitwise": 2,
    "no-buffer-constructor": "error",
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-catch-shadow": 2,
    "no-class-assign": 2,
    "no-compare-neg-zero": "error",
    "no-cond-assign": 2,
    "no-confusing-arrow": 2,
    "no-console": 0,
    "no-const-assign": 2,
    "no-constant-condition": 0,
    "no-continue": 0,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 0,
    "no-empty-character-class": 2,
    "no-empty-function": 0,
    "no-empty-pattern": 2,
    "no-empty": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 0,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-label": 0,
    "no-extra-parens": [2, "functions"],
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-global-assign": 0,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-invalid-this": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-misleading-character-class": 0,
    "no-mixed-operators": 0,
    "no-mixed-requires": [2, false],
    "no-mixed-spaces-and-tabs": [2, false],
    "no-multi-assign": "error",
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [2, { "max": 2 }],
    "no-native-reassign": 2,
    "no-negated-condition": 0,
    "no-negated-in-lhs": 0,
    "no-nested-ternary": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 0,
    "no-new-symbol": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-obj-calls": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-path-concat": 0,
    "no-plusplus": 0,
    "no-process-env": 0,
    "no-process-exit": 1,
    "no-proto": 2,
    "no-prototype-builtins": 0,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-reserved-keys": 0,
    "no-restricted-globals": 0,
    "no-restricted-imports": 0,
    "no-restricted-modules": 0,
    "no-restricted-properties": 0,
    "no-restricted-syntax": [0, "FunctionExpression"],
    "no-return-assign": 2,
    "no-return-await": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 1,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-sync": 0,
    "no-tabs": 2,
    "no-template-curly-in-string": 0,
    "no-ternary": 0,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unmodified-loop-condition": 0,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 0,
    "no-unsafe-negation": 0,
    "no-unused-expressions": 2,
    "no-unused-labels": 0,
    "no-unused-vars": [2, { "args": "after-used", "vars": "all" }],
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-useless-catch": 0,
    "no-useless-computed-key": 0,
    "no-useless-concat": 0,
    "no-useless-constructor": 0,
    "no-useless-escape": 2,
    "no-useless-rename": 2,
    "no-useless-return": 2,
    "no-var": 2,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "nonblock-statement-body-position": "off",
    "object-curly-newline": 0,
    "object-curly-spacing": [2, "always"],
    "object-property-newline": 0,
    "object-shorthand": 0,
    "one-var-declaration-per-line": [2, "always"],
    "one-var": [2, "never"],
    "operator-assignment": [0, "always"],
    "operator-linebreak": [2, "after"],
    "padded-blocks": 0,
    "padding-line-between-statements": "off",
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-destructuring": [
      "error",
      { "array": false, "object": true },
      { "enforceForRenamedProperties": true }
    ],
    "prefer-let/prefer-let": 2,
    "prefer-named-capture-group": 0,
    "prefer-numeric-literals": 0,
    "prefer-object-spread": 0,
    "prefer-promise-reject-errors": "error",
    "prefer-reflect": 2,
    "prefer-rest-params": 0,
    "prefer-spread": 2,
    "prefer-template": 0,
    "quote-props": [2, "as-needed"],
    "quotes": [2, "double"],
    "radix": 2,
    "require-atomic-updates": 0,
    "require-await": "error",
    "require-jsdoc": 0,
    "require-unicode-regexp": 0,
    "require-yield": 2,
    "rest-spread-spacing": 0,
    "semi-spacing": [2, { "after": true, "before": false }],
    "semi-style": ["error", "last"],
    "semi": 2,
    "sort-imports": 0,
    "sort-keys": 0,
    "sort-vars": 0,
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": 0,
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, { "nonwords": false, "words": true }],
    "spaced-comment": [2, "always"],
    "strict": 0,
    "switch-colon-spacing": "error",
    "symbol-description": 0,
    "template-curly-spacing": 0,
    "template-tag-spacing": "error",
    "unicode-bom": 0,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "vars-on-top": 2,
    "wrap-iife": [2, "outside"],
    "wrap-regex": 0,
    "yield-star-spacing": ["error", "before"],
    "yoda": [2, "never"]
  }
}
